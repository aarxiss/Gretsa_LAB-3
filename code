#include <iostream>
using namespace std;

int main()
{
    setlocale(LC_ALL, "uk_UA");
    int n1, n2;
    int NSD;
    cout << "Введіть два числа:" << endl;
    cin >> n1 >> n2;
    do {
        NSD = n1 % n2;
        n1 = n2;
        n2 = NSD;
    } while (NSD != 0);
    cout << "Найбільший спільний дільник:" << n1 << endl;



    return 0;
}
